<Application x:Class="Raminagrobi.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Raminagrobi.WPF"
             xmlns:viewModels="clr-namespace:Raminagrobi.WPF.ViewModels"
             xmlns:views="clr-namespace:Raminagrobi.WPF.Views"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:HomeViewModel}">
            <views:Home/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:CartViewModel}">
            <views:Cart/>
        </DataTemplate>
    </Application.Resources>
</Application>
